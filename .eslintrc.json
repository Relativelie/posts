{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "import/extensions": "off",
        "array-callback-return": "off",
        "no-plusplus": "off",
        "import/prefer-default-export": "off",
        "no-use-before-define": "off"
    }
}
